<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>CSP</display-name>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>cn.edu.njust.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>StudentModifyServlet</servlet-name>
    <servlet-class>cn.edu.njust.servlet.StudentModifyServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>StudentDeleteServlet</servlet-name>
    <servlet-class>cn.edu.njust.servlet.StudentDeleteServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ScoreModifyServlet</servlet-name>
    <servlet-class>cn.edu.njust.servlet.ScoreModifyServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ScoreDeleteServlet</servlet-name>
    <servlet-class>cn.edu.njust.servlet.ScoreDeleteServlet</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/servlet/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StudentModifyServlet</servlet-name>
    <url-pattern>/servlet/StudentModifyServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StudentDeleteServlet</servlet-name>
    <url-pattern>/servlet/StudentDeleteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ScoreModifyServlet</servlet-name>
    <url-pattern>/servlet/ScoreModifyServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ScoreDeleteServlet</servlet-name>
    <url-pattern>/servlet/ScoreDeleteServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>